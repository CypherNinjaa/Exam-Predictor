{
	"name": "exam-predictor",
	"version": "1.0.0",
	"description": "AI-powered exam question predictor using Gemini 3.0 for Amity University Patna",
	"main": "index.js",
	"scripts": {
		"dev": "next dev",
		"build": "prisma generate && next build",
		"start": "next start",
		"lint": "next lint",
		"postinstall": "prisma generate",
		"db:generate": "prisma generate",
		"db:push": "prisma db push",
		"db:migrate": "prisma migrate dev",
		"db:migrate:deploy": "prisma migrate deploy",
		"db:studio": "prisma studio",
		"db:seed": "npx ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
		"extract:pdf": "npx ts-node --compiler-options {\"module\":\"CommonJS\"} src/scripts/extract-pdf.ts",
		"extract:syllabus": "npx ts-node --compiler-options {\"module\":\"CommonJS\"} src/scripts/extract-syllabus.ts",
		"test": "jest"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"dependencies": {
		"@clerk/nextjs": "^6.35.5",
		"@clerk/themes": "^2.4.40",
		"@google/generative-ai": "^0.24.1",
		"@prisma/client": "^5.22.0",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-tabs": "^1.1.13",
		"@radix-ui/react-toast": "^1.2.15",
		"axios": "^1.7.7",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"dotenv": "^16.4.5",
		"framer-motion": "^12.23.24",
		"lucide-react": "^0.555.0",
		"next": "^14.2.15",
		"pdf-parse": "^1.1.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-dropzone": "^14.3.8",
		"tailwind-merge": "^3.4.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@types/node": "^22.9.0",
		"@types/pdf-parse": "^1.1.4",
		"@types/react": "^18.3.12",
		"autoprefixer": "^10.4.20",
		"eslint": "^8.57.1",
		"eslint-config-next": "^14.2.15",
		"postcss": "^8.4.47",
		"prisma": "^5.22.0",
		"tailwindcss": "^3.4.14",
		"ts-node": "^10.9.2",
		"typescript": "^5.6.3"
	},
	"keywords": [
		"exam-predictor",
		"ai",
		"gemini",
		"prisma",
		"next.js",
		"amity-university"
	],
